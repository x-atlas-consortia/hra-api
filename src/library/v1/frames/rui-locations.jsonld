      },
      "y_dimension": {
        "@id": "ccf:y_dimension",
        "@type": "xsd:integer"
      },
      "z_dimension": {
        "@id": "ccf:z_dimension",
        "@type": "xsd:integer"
      },
      "x_scaling": {
        "@id": "ccf:x_scaling",
        "@type": "xsd:integer"
      },
      "y_scaling": {
        "@id": "ccf:y_scaling",
        "@type": "xsd:integer"
      },
      "z_scaling": {
        "@id": "ccf:z_scaling",
        "@type": "xsd:integer"
      },
      "x_rotation": {
        "@id": "ccf:x_rotation",
        "@type": "xsd:integer"
      },
      "y_rotation": {
        "@id": "ccf:y_rotation",
        "@type": "xsd:integer"
      },
      "z_rotation": {
        "@id": "ccf:z_rotation",
