      "rui_location": "ccf:has_registration_location",
      "sections": "ccf:subdivided_into_sections",
      "annotations": {
        "@id": "ccf:annotations",
        "@type": "@id"
      }
    }
  ],
  "@type": "ccf:donor",
  "samples": {
    "@type": "ccf:sample",
    "rui_location": {
      "@type": "ccf:SpatialEntity",
